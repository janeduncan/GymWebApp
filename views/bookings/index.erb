<h1>All Bookings</h1><br>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Class</th>
      <th>Time</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% @bookings.each do |booking| %>
    <tr>
      <td><a href="/bookings/<%= booking.id %>"><b><%= booking.member.full_name() %></b></a></td>
      <td><%= booking.gym_session.gymclass() %></td>
      <td><%= booking.gym_session.pretty_time() %></td>
      <td><%= booking.gym_session.class_date() %></td>
    </tr>
    <% end %>
  </tbody>
</table>
